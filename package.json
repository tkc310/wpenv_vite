{
  "name": "wpenv_vite",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "22.11.0",
    "npm": "10.9.x"
  },
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "wp:start": "wp-env start",
    "wp:stop": "wp-env stop",
    "wp:export": "npx wp-env run cli wp db export sql/backup.sql",
    "wp:import": "npx wp-env run cli wp db reset --yes && wp-env run cli wp db import sql/backup.sql",
    "tsc": "tsc --noEmit ",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:style": "stylelint \"**/*.{scss,css}\"",
    "lint:style:fix": "stylelint --fix \"**/*.{scss,css}\"",
    "prettier": "prettier --write \"**/*.{html,scss,ts,js,php}\" --ignore-path .gitignore"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "5.1.1",
    "@eslint/js": "9.14.0",
    "@prettier/plugin-php": "0.22.2",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "@typescript-eslint/parser": "8.14.0",
    "@wordpress/env": "10.12.0",
    "autoprefixer": "10.4.20",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "glob": "11.0.0",
    "globals": "15.12.0",
    "lefthook": "1.8.2",
    "postcss": "8.4.49",
    "postcss-scss": "4.0.9",
    "prettier": "3.3.3",
    "stylelint": "16.10.0",
    "stylelint-config-recess-order": "5.1.1",
    "stylelint-config-standard-scss": "13.1.0",
    "typescript": "5.6.3",
    "typescript-eslint": "8.14.0",
    "vite": "5.4.11",
    "vite-plugin-full-reload": "1.2.0",
    "vite-tsconfig-paths": "5.1.2"
  },
  "dependencies": {
    "tailwindcss": "3.4.17"
  }
}
